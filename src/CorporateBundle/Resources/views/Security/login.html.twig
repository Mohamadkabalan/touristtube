{% extends 'basecorporateFinal.html.twig' %}

{% block pagenewcss %}
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/corporate/css/corporate-signin_new.css")|raw }}" >
{% endblock %}
{% block pagenewjs %}
    <script type="text/javascript" src="{{ asset("/assets/corporate/js/corporate-login.js")|raw }}"></script>
{% endblock %}
{% block title %}{{'Login'|trans}} Tourist Tube{% endblock %}
{% block body %}
    <section class="container corprate_newdesign_container">
        <div class="row no-margin">
            <div class="col-xs-12 nopad">
                <div class="row no-margin margin_top_20 margin_bottom_40">
                    <div class="col-xs-12 nopad">
                        <h1 class="search_title"><span class="circlimg_arrow"><i class="fas fa-angle-right"></i></span>{{'Corporate'|trans}} | <span>{{'Login'|trans}}</span> </h1>
                    </div>
                </div>
                <div class="row no-margin bordered_row"></div>
            </div>
	    <div class="col-xs-12 nopad">
		<div class="row no-margin margin_top_40 margin_bottom_40 logincontainer">
		    <form action="{{ path('login') }}" method="post">
			<div class="col-xs-12 nopad">
			    <div class="row">
				<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
				    <label class="">{{'Email address / username'|trans}}*</label>				    
				    <input type="text" id="username" name="_username" value="{{ last_username }}" placeholder="{{'Email address / username'|trans}}" class="default_input required" />
				</div>
				<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
				    <label class="">{{'Password'|trans}}*</label>				    
				    <input type="password" id="password" name="_password" placeholder="{{'Password'|trans}}*" class="default_input required" />
				    <input type="hidden" name="_target_path" value="/corporate" />
				    <input type="hidden" id="latitude" name="_latitude" value="" />
				    <input type="hidden" id="longitude" name="_longitude" value="" />
				    <input type="hidden" id="resetPasswordPath" value="{{ generateLangRoute(LanguageGet,'_reset_password_corporate') }}" />
				    <p class="forgetpass_link forgetbutton">{{'forgot password?'|trans}}</p>
				</div>
			    </div>
			    <div class="row">
				<div class="col-xs-12">
				    <div class="checkbox">
					<label for="remember_me" class="checkboxlabel"><input id="remember_me" type="checkbox" name="_remember_me" class="checkboxlabelinput" checked> {{'Keep me logged in'|trans}}</label>					
				    </div>
				</div>
			    </div>
			    <div class="row">
				<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
				    <button id="LogInSignIn" type="button" class="btn submit_yellow">{{'Sign in'|trans}}</button>
				</div>
			    </div>
			    <div class="row">
				<div class="col-xs-12 margin_top_10">
				    {% if error %}
					<label class="login_error_msg">{{ error.messageKey|trans(error.messageData, 'security') }}</label>
				    {% elseif app.request.get('validation_error') %}
					<label class="login_error_msg">{{'Inactive user'|trans}}</label>
				    {% endif %}
				</div>
			    </div>
			    <div class="row">
				<div class="col-xs-12 margin_top_10">
				    <label class="wrong_credentialspass wrong_credentialsmsg"></label>
				</div>
			    </div>
			    <div class="row">
				<div class="col-xs-12 margin_top_10">
				    <label id="loginErr" class="wrong_credentials">{{'invalid credentials'|trans}}</label>
				</div>
			    </div>
			</div>
		    </form>
		</div>
		<div class="row no-margin margin_top_40 margin_bottom_40 forgot_your_password">
		    <div class="col-xs-12 nopad">
			<div class="row">
			    <div class="col-sm-6 col-xs-12">
				<label class="">{{'Enter your email or username'|trans}}*</label>
				<input id="emailforget" placeholder="{{'Enter your email or username'|trans}}*" class="default_input required" type="email">
			    </div>
			</div>
			<div class="row">
			    <div class="col-xs-12 margin_top_10">
				<button type="button" class="btn submit_yellow SendForgot">{{'Reset'|trans}}</button>
				<button id="" type="button" class="back_grey CancelForgot">{{'Cancel'|trans}}</button>
			    </div>
			</div>
			<div class="row">
			    <div class="col-xs-12 margin_top_10">
				<label class="wrong_credentialspass wrong_credentialsmsg"></label>
			    </div>
			</div>
			<div class="row">
			    <div class="col-xs-12 margin_top_10">
				<label class="wrong_credentials">{{'invalid credentials'|trans}}</label>
			    </div>
			</div>
		    </div>
		</div>
	    </div>
            <div class="col-xs-12 nopad">
                <div class="row no-margin margin_top_20 margin_bottom_40">
                    <div class="col-xs-12 nopad">
                        <p class="common_grey_text">{{'We can provide your company swiftly with adapted travel plans while we optimize your corporate travel budget.'|trans}}</p>
                        <p class="common_grey_text">{{'Our travel experts are always set and ready to bring you quality services and utmost convenience during your business trips.'|trans}}</p>
                    </div>
                </div>
                <div class="row no-margin">
                    <div class="col-xs-12 nopad">
                        <p class="common_black_text padding_left_15">{{'Tourist Tube offers you the following Corporate Services:'|trans}}</p>
                    </div>
                    <div class="col-xs-12 nopad">
                        <ul class="signin_corporate_list">
                            <li>{{'International and domestic air ticket booking, hotel reservations and car rental'|trans}}</li>
                            <li>{{'Best Deals for corporate domestic and international agreements with airlines and hotel aggregators'|trans}}</li>
                            <li>{{'Cruise / Holiday / Leisure / Activities & Events'|trans}}</li>
                            <li>{{'Inbound and outbound group tours and MICE (Meetings, incentives, conferences and exhibitions)'|trans}}</li>
                            <li>{{'Overseas medical policy and travel insurance'|trans}}</li>
                            <li>{{'Meet and greet services at selected airports'|trans}}</li>
                            <li>{{'Travel desk implementation services'|trans}}</li>
                            <li>{{'Offshore ticketing from our multiple PCCs'|trans}}</li>
                            <li>{{'Visa and Passport facilitation and Foreign exchange services'|trans}}</li>
                            <li>{{'Tourist Tube\'s Miles (reward points) on every domestic airline booking that could be redeemed for discounts'|trans}}</li>
                            <li>{{'Tourist Tube\'s own online Corporate Booking Tool'|trans}}</li>
                            <li>{{'Service Level Agreements'|trans}}</li>
                            <li>{{'Dedicated customer care service and corporate relationship management'|trans}}</li>
                            <li>{{'Quarterly review meets / service camps / feedback'|trans}}</li>
                        </ul>
                    </div>
                    <div class="col-xs-12 nopad margin_top_20">
                        <p class="common_black_text">{{'For our specialists to serve you best, kindly fill up the form below and our corporate travel consultants will contact you personally.'|trans}}
                            {{'For your inquiries, contact us at +'|trans}}
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock %}

