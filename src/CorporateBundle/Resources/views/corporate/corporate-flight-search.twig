{% extends 'basecorporate.html.twig' %}

{% block pagenewcss %}
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/6.1.3/css/bootstrap-slider.css"/>
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/corporate/css/booking_steps.css")|raw }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/flights/css/flight-booking-result.css")|raw }}" > 
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/common/css/daterangepicker.min.css")|raw }}" >
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/corporate/css/corporate-flight_theme"~corporate_theme~".css")|raw }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/corporate/css/corporate-flightandmenue.css")|raw }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/corporate/css/corporate-flight-search.css")|raw }}"/>
{% endblock %}
{% block pagenewjs %}
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/6.1.3/bootstrap-slider.js"></script>
    <script type="text/javascript" src="/assets/flights/js/flight-booking-result-ui.js"></script>
    <script type="text/javascript" src="{{ asset("/assets/flights/js/isotope.pkgd.min.js")|raw }}"></script>
    <script type="text/javascript" src="{{ asset("/assets/corporate/js/corporate-flightandmenue.js")|raw }}"></script>
    <script type="text/javascript" src="{{ asset("/assets/corporate/js/corporate-flight-search.js")|raw }}"></script>
    <script type="text/javascript" src="{{ asset("/assets/common/js/moment.min.js") }}"></script>
    <script type="text/javascript" src="{{ asset("/assets/common/js/jquery.daterangepicker.js") }}"></script>
    <script type="text/javascript" src="{{ asset("/assets/flights/js/flight-booking.js")|raw }}"></script>
    <script type="text/javascript" src="{{ asset("/assets/common/js/jquery.daterangepicker.js") }}"></script>

    <script>
        //window.SKIP_AUTO_LOAD_COOKIES_SEARCH_FORM = true;
    </script>
{% endblock %}
{% block title %}{{'Flight Search'|trans}}{% endblock %}

{% block body %} 
    {% set page_menu_name = 'flights' %}
    {% set booking_steps=3 %}
    <section class="container corporate_container1">
        <div class="col-xs-12 nopad">
            <div class="row rowspecialstyle">
                <div class="col-xs-12 nopad containerstyle_hbr">
                    <div class="col-lg-2 col-md-3 col-sm-12 col-xs-12 nopad leftmenuebackcolor_corp borderrightgrey_corpfl">
                        <div class="col-xs-12 nopad">
                            {% include '@Corporate/corporate/corporate-leftmenu.twig' %}
                        </div>
                        <div class="col-xs-12 filter-bordertop nopad">
                            <div class="col-xs-12 menuestyle_filter-click nopad">
                                <div class="col-xs-12 nopad">
                                    <img class="downarrow_filter" src="{{ generateMediaURL('/media/images/corporate/downarrow_menuefilters.png') }}" alt="{{'title'}}"/>
                                </div>
                                <div class="col-xs-12 nopad">
                                    <p class="menuestyle_filter">{{'filter results'|trans}}</p>
                                </div>
                            </div>
                            <div class="col-xs-12 filtersearchtogglecontainer nopad">
                                <div class="col-xs-12 greybordertopdivfilter left-bar padding0 nopad">
                                    <div class="fly-filter col-xs-12">
                                        <div class="block-title col-xs-12">
                                            <h1>{{'Filters'|trans}}</h1>
                                        </div>
                                        <div class="col-xs-12 accordion">
                                            <div class="col-lg-12 col-md-12 col-sm-6 col-xs-12">
                                                {% if not multi_destination %}
                                                    <div class="accordion-section">
                                                        <div class="block_r leaving-f">
                                                            <div class="title_with_icon">
                                                                <div class="bg fl"></div>
                                                                <h2 class="txt fl  accordion-section-title">{{'leaving flight'|trans}}</h2>
                                                                <div class="clearfix"></div>
                                                            </div>
                                                            <div class=" accordion-section-content">
                                                                <div class="wording" data-filter-group="slider-time-1">
                                                                    <label for="amount-time-1">{{'depart'|trans}}</label>
                                                                    <input class="slider-range" type="text" name="amount-time-1" id="amount-time-1" value="" readonly/>
                                                                    <div id="slider-time-1" data-filter="leavingDeparture"></div>
                                                                </div>
                                                                <div class="wording" data-filter-group="slider-time-2">
                                                                    <label for="amount-time-2">{{'arrive'|trans}}</label>
                                                                    <input class="slider-range" type="text" name="amount-time-2" id="amount-time-2" value="" readonly/>
                                                                    <div id="slider-time-2" data-filter="leavingArrival"></div>
                                                                </div>
                                                                <div class="wording" data-filter-group="slider-time-3">
                                                                    <label for="amount-time-3">{{'duration'|trans}}</label>
                                                                    <input class="slider-range" type="text" name="amount-time-3" id="amount-time-3" value="" readonly/>
                                                                    <div id="slider-time-3" data-min-duration="{{ minimum_duration|raw }}" data-max-duration="{{ maximum_duration|raw }}" data-filter="leavingDuration"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    {% if not one_way %}
                                                        <div class="accordion-section">
                                                            <div class="block_r returnning-f">
                                                                <div class="title_with_icon">
                                                                    <div class="bg fl"></div>
                                                                    <h2 class="txt fl accordion-section-title">{{'returning flight'|trans}}</h2>
                                                                    <div class="clearfix"></div>
                                                                </div>
                                                                <div class=" accordion-section-content">
                                                                    <div class="wording" data-filter-group="slider-time-4">
                                                                        <label for="amount-time-4">{{'depart'|trans}}</label>
                                                                        <input class="slider-range" type="text" name="amount-time-4" id="amount-time-4" value="" readonly/>
                                                                        <div id="slider-time-4" data-filter="returningDeparture"></div>
                                                                    </div>
                                                                    <div class="wording" data-filter-group="slider-time-5">
                                                                        <label for="amount-time-5">{{'arrive'|trans}}</label>
                                                                        <input class="slider-range" type="text" name="amount-time-5" id="amount-time-5" value="" readonly/>
                                                                        <div id="slider-time-5" data-filter="returningArrival"></div>
                                                                    </div>
                                                                    <div class="wording" data-filter-group="slider-time-6">
                                                                        <label for="amount-time-6">{{'duration'|trans}}</label>
                                                                        <input class="slider-range" type="text" name="amount-time-6" id="amount-time-6" value="" readonly/>
                                                                        <div id="slider-time-6" data-min-duration="{{ minimum_duration|raw }}" data-max-duration="{{ maximum_duration|raw }}" data-filter="returningDuration"></div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    {% endif %}
                                                {% endif %}
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-6 col-xs-12">
                                                <div class="accordion-section">
                                                    <div class="block_r price-f">
                                                        <div class="title_with_icon">
                                                            <div class="bg fl"></div>
                                                            <h2 class="txt fl accordion-section-title"> <label for="amount">{{'Price'|trans}}</label></h2>
                                                            <div class="clearfix"></div>
                                                        </div>
                                                        <div class=" accordion-section-content">
                                                            <div class="wording" data-filter-group="price-group">
                                                                <p>
                                                                    <input class="slider-range withstyle" type="text" id="amount" readonly>
                                                                </p>
                                                                <div id="price-range" data-min-price="{{ minimum_price|raw }}" data-max-price="{{ maximum_price|raw }}" data-filter="priceFilter"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="accordion-section">
                                                    <div class="block_r stops-f">
                                                        <div class="title_with_icon">
                                                            <div class="bg fl"></div>
                                                            <div class="txt fl accordion-section-title">{{'stops'|trans}}</div>
                                                            <div class="clearfix"></div>
                                                        </div>
                                                        <div class=" accordion-section-content">
                                                            <div class="wording" data-filter-group="stops-group">
                                                                <p>
                                                                    <input class="slider-range" type="text" id="amount-1" readonly>
                                                                </p>
                                                                <div id="slider-range-2" data-filter="stopsFilter"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="filters">
                                                    <div class="block_r refundable-wrapper">
                                                        <div id="bestCombination" class="input-group checkbox" data-filter-group="bestCombination">
                                                            <label for="bestCombinationFilter"><input id="bestCombinationFilter" type="checkbox" value="" data-filter=".bestcombination"/>{{'best combination'|trans}}</label>
                                                        </div>
                                                        <div id="refundableGroup" class="input-group checkbox" data-filter-group="refundable">
                                                            <label for="refundableFilter"><input id="refundableFilter" type="checkbox" value="" data-filter=".refundable"/>{{'refundable only'|trans}}</label>
                                                        </div>
                                                    </div>

                                                    <div class="accordion-section">
                                                        <div class="block_r airline-f ">
                                                            <div class="title_with_icon">
                                                                <div class="bg fl"></div>
                                                                <div class="txt fl accordion-section-title">{{'airlines'|trans}}</div>
                                                                <div class="clearfix"></div>
                                                            </div>
                                                            <div class=" accordion-section-content nopad">
                                                                <div id="airlinesGroup" class="checkbox input-group" data-filter-group="airlines-group">
                                                                    {% for key, airline in airlines %}
                                                                        <label for="{{ key|raw }}">
                                                                            <input id="{{ key|raw }}" type="checkbox" value="{{ key|raw }}" data-filter=".{{ key|raw }}">{{ airline.name|raw }} <span class="a_price price-convert" data-price="{{ airline.amount_attr|raw }}"><span class="currency-convert-text">{{currency_code|raw}}</span> <span class="price-convert-text">{{ airline.amount|raw }}</span></span>
                                                                        </label>
                                                                    {% endfor %}
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 uparrow_filter-click nopad">
                                    <img class="uparrow_filter" src="{{ generateMediaURL('/media/images/corporate/uparrow_menuefilters.png') }}" alt="{{'title'}}"/>
                                </div>
                            </div>

                        </div>

                    </div>
                    <div class="col-lg-8 col-md-9 col-sm-12 col-xs-12 nopad contentbackcolor_corp">
                        <div class="col-xs-12 nopad">
                            <div class="col-xs-12 nopad">
                                <div id="currencyTab" class="fixposbout fixposboutNew">
                                    <div class="arrowbluediv">
                                        <img class="imagarrowblue" src="{{ generateMediaURL('/media/images/whiteuparr.png') }}" alt="{{'white arrow'|trans}}" />
                                    </div>

                                </div>

                                <h2 class="headerstyle_corp">{{'step2'|trans}} <span class="error_cnt_corporate"></span></h2>

                                <div class="col-xs-12 nopad">		    	
                                    <div class="searchButtonsUpdate changeSearch">{{'change search'|trans}}</div>
                                    <div class="searchButtonsUpdate cancelSearch">{{'cancel'|trans}}</div>
                                </div>
                                <div class="col-xs-12 nopad formsearch">		    	
                                    {% include '@Corporate/corporate/flight-form.twig' %}
                                </div>
                                <div class="col-xs-12">
                                    <div class="col-xs-12">
                                        {% include '@Corporate/corporate/booking_steps.twig' %}
                                    </div>
                                </div>
                                <div class="col-xs-12">
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 nopadtenblue">
                                        <div class="bluebacksearchdiv">
                                            <p class="textsearchblue">{{'order by'|trans}}</p>

                                        </div>
                                    </div>
                                    <div class="floatinginput inputbox col-lg-2 col-md-2 col-sm-2 col-xs-4 xsnolp">
                                        <select id="sortDuration" class="sortingSelect">
                                            <option data-sort-value="" value="" selected>{{'Duration'|trans}}</option>
                                            <option data-sort-value="shortestDuration" value="shortestDuration">{{'Shortest Duration'|trans}}</option>
                                            <option data-sort-value="longestDuration" value="longestDuration">{{'Longest Duration'|trans}}</option>
                                        </select> 
                                    </div>
                                    <div class="floatinginput inputbox col-lg-2 col-md-2 col-sm-2 col-xs-4 ">
                                        <select id="sortStops" class="sortingSelect">
                                            <option data-sort-value="" value="" selected>{{'Stops'|trans}}</option>
                                            <option data-sort-value="minimumStops" value="minimumStops">{{'Minimum Stops'|trans}}</option>
                                            <option data-sort-value="maximumStops" value="maximumStops">{{'Maximum Stops'|trans}}</option>
                                        </select>
                                    </div>
                                    <div class="floatinginput inputbox col-lg-2 col-md-2 col-sm-2 col-xs-4 xsnorp">
                                        <select id="sortPrice" class="sortingSelect">
                                            <option data-sort-value="" value="" selected>{{'Price'|trans}}</option>
                                            <option data-sort-value="cheapestPrice" value="cheapestPrice">{{'Cheapest Price'|trans}}</option>
                                            <option data-sort-value="highestPrice" value="highestPrice">{{'Highest Price'|trans}}</option>
                                        </select>
                                    </div>
                                    <div class="floatinginput inputbox col-lg-3 col-md-3 col-sm-3 col-xs-4 xspadleft0">
                                        <select id="sortEarliestTime" class="sortingSelect">
                                            <option data-sort-value="" value="" selected>{{'Time'|trans}}</option>
                                            <option data-sort-value="earliestDeparture" value="earliestDeparture">{{'Earliest Departure'|trans}}</option>
                                            <option data-sort-value="earliestArrival" value="earliestArrival">{{'Earliest Arrival'|trans}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-xs-12 visible-xs nopad">
                                    <p class="ressmal">{{'RESULTS'|trans}}</p>
                                </div>
                                <div class="col-lg-12 col-md-12 col-sm-12 nopad">
                                    <div class="col-lg-12 col-md-12 col-sm-12 hidden-xs nopad">
                                        <div class="blackheader">
                                            <ul class=" col-xs-12 ulheadstyle">
                                                <li class="airlinestyle">{{'Airline'|trans}}</li>
                                                <li class="departuredate">{{'Departure'|trans}}</li>
                                                <li class="stpsstyle">{{'Stops'|trans}}</li>
                                                <li class="othercommonstyle">{{'Arrival'|trans}}</li>
                                                <li class="durationst">{{'Duration'|trans}}</li>
                                                <li class="othercommonstyleprice">{{'Price'|trans}}</li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 rightcontainercolor_corpfl">
                                        <div class="grid col-xs-12 nopad">
                                            {% if error is defined %}
                                                <div class="alert-danger">{{error|trans}}</div>
                                            {% else %}
                                                {%if errors is defined %}
                                                    <div class="alert-danger">
                                                        <ul>
                                                            {% for message in errors %}
                                                                <li>
                                                                    {{message|raw}}
                                                                </li>
                                                            {% endfor %}
                                                        </ul>
                                                    </div>
                                                {% else %}
                                                    {%if segmentsArray is not empty %}
                                                        {% for segmentsItem in segmentsArray %}
                                                            {% set segmentsItemIndex = loop.index %}
                                                            <div class="fly {{ segmentsItem.non_refundable_css_class|raw }} {{ segmentsItem.marketing_airline|raw }} {{ segmentsItem.segmentsGlob.classExactDate|raw }}">
                                                                <form id="book-{{loop.index}}" action="{{ path('_corporate_book_flight') }}" method="POST" autocomplete="off">
                                                                    <input type="hidden" name="number_in_party" value="{{ num_in_party|raw }}" />
                                                                    <input type="hidden" name="original_price" value="{{segmentsItem.segmentsGlob.original_price|raw}}" />
                                                                    <div class="price-convert" data-price="{{segmentsItem.segmentsGlob.price_attr|raw}}"><input class="price-convert-val" type="hidden" name="price_attr" value="{{segmentsItem.segmentsGlob.price_attr|raw}}" /></div>
                                                                    <div class="price-convert" data-price="{{segmentsItem.segmentsGlob.price|raw}}"><input class="price-convert-val" type="hidden" name="price" value="{{segmentsItem.segmentsGlob.price|raw}}" /></div>
                                                                    <input type="hidden" name="currency" value="{{ currency|raw }}" />
                                                                    <div class="price-convert"><input class="currency-convert-val" type="hidden" name="currency_code" value="{{ currency_code|raw }}" /></div>
                                                                    <input type="hidden" name="multi_destination" value="{{ multi_destination|raw }}" />
                                                                    <input type="hidden" name="one_way" value="{{ one_way|raw }}" />

                                                                    <div class="col-xs-12 nopad hidden-xs">
                                                                        <div class="leaving-stops col-lg-12 col-md-12 col-sm-12 nopad" 
                                                                             data-datedeparttureobj="{{segmentsItem.segmentsGlob.departure_lowest_date_time_obj.format('M d, Y G:i:s')}}"
                                                                             data-datearrivaltureobj="{{segmentsItem.segmentsGlob.arrival_lowest_date_time_obj.format('M d, Y G:i:s')}}" 
                                                                             data-stops="{{segmentsItem.segmentsGlob.stops|raw}}" 
                                                                             data-earliestdeparture="{{segmentsItem.segmentsGlob.departure_lowest_timestamp}}" 
                                                                             data-earliestarrival="{{segmentsItem.segmentsGlob.arrival_lowest_timestamp}}">

                                                                            <ul class=" col-xs-12 ulheadstyle2 displayli flyminHeight">
                                                                                <li class="airlinestyle2 newstyleairline">
                                                                                    <ul>
                                                                                        <li> <img class="chtimg" alt="{{segmentsItem.segmentsGlob.airline_name|raw}}" src="{{segmentsItem.segmentsGlob.airline_logo|raw}}"/></li>
                                                                                        <li>{{segmentsItem.segmentsGlob.airline_name|raw}}<br>{{segmentsItem.segmentsGlob.airline|raw}}{{segmentsItem.segmentsGlob.flight_number|raw}}</li>
                                                                                    </ul>
                                                                                </li>
                                                                                <li class="departuredate newdep leaving-departure-time-minutes" data-time-minutes="{{segmentsItem.segmentsGlob.departure_time_minutes|raw}}">{{segmentsItem.segmentsGlob.departure_date_time|raw}}{#<span class="datenumbclass"> 30 Dec</span> 23:10#}</li>
                                                                                <li class="stpsstyle newstops"><img alt="{{'stops'|trans}}" class="" src="{{ generateMediaURL('/media/images/stpsimg.png') }}" /><br> <span class="destpadr">{{segmentsItem.segmentsGlob.departure_airport_code|raw}}</span> <span>{{segmentsItem.segmentsGlob.arrival_airport_code|raw}}</span></li>
                                                                                <li class="othercommonstyle newarrives leaving-arrival-time-minutes" data-time-minutes="{{segmentsItem.segmentsGlob.arrival_time_minutes|raw}}">{{segmentsItem.segmentsGlob.arrival_date_time|raw}}{#<span class="hourstyle"> 16:00</span><br><span class="plustime">+1 day</span>#}</li>
                                                                                <li class="durationst newduration leaving-duration" data-flight-duration="{{ segmentsItem.segmentsGlob.flight_duration_attr|raw }}"><span class="sizespan">{{segmentsItem.segmentsGlob.flight_duration|raw}}</span> <span class="morestyle" >{{'more'|trans}}</span><span class="lessstyle" >{{'less'|trans}}</span></li>
                                                                                <li class="flight-price othercommonstyleprice newprice price-convert" data-price="{{segmentsItem.segmentsGlob.price_attr|raw}}"><span class="currency-convert-text">{{currency_code|raw}}</span> <span class="price-convert-text">{{segmentsItem.segmentsGlob.price|raw}}</span><br/><span class="seats_left">{{segmentsItem.seats_remaining|raw}} {{'left'|trans}}</span>{#<span class="priceclass">AED</span><span class="numbprice">4.346</span>#}</li>
                                                                                <li class="bookingstyle"><button class="submit-booking" type="submit" name="submit-booking" >{{'BOOK'|trans}}</button></li>
                                                                            </ul>
                                                                        </div>

                                                                        {% if (one_way == 1 and multi_destination == 1) or (one_way == 0 and multi_destination == 0) %}
                                                                            {% for segmentsItemMulti in segmentsItem.segmentsGlob.segmentsGlobMulti %}
                                                                                <div class="returning-stops col-lg-12 col-md-12 col-sm-12 nopad" data-stops="{{segmentsItemMulti.stops1|raw}}">
                                                                                    <ul class=" col-xs-12 ulheadstyle2 displayli">
                                                                                        <li class="airlinestyle2 newstyleairline">
                                                                                            <ul>
                                                                                                <li> <img class="chtimg" alt="{{segmentsItemMulti.airline_name1|raw}}" src="{{segmentsItemMulti.airline_logo1|raw}}"/></li>
                                                                                                <li>{{segmentsItemMulti.airline_name1|raw}}<br>{{segmentsItemMulti.airline1|raw}}{{segmentsItemMulti.flight_number1|raw}}</li>
                                                                                            </ul>
                                                                                        </li>
                                                                                        <li class="departuredate newdep returning-departure-time-minutes" data-time-minutes="{{segmentsItemMulti.departure_time_minutes1|raw}}">{{segmentsItemMulti.departure_date_time1|raw}}</li>
                                                                                        <li class="stpsstyle newstops"><img class="" src="{{ generateMediaURL('/media/images/stpsimg.png') }}" alt="{{'stops'|trans}}"/><br> <span class="destpadr">{{segmentsItemMulti.departure_airport_code1|raw}}</span> <span>{{segmentsItemMulti.arrival_airport_code1|raw}}</span></li>
                                                                                        <li class="othercommonstyle newarrives returning-arrival-time-minutes" data-time-minutes="{{segmentsItemMulti.arrival_time_minutes1|raw}}">{{segmentsItemMulti.arrival_date_time1|raw}}</li>
                                                                                        <li class="durationst newduration returning-duration" data-flight-duration="{{ segmentsItemMulti.flight_duration_attr1|raw }}">{{segmentsItemMulti.flight_duration1|raw}}</li>
                                                                                    </ul>
                                                                                </div>
                                                                            {% endfor %}
                                                                        {% endif %}

                                                                        <div class="closedborder col-lg-12 col-md-12 col-sm-12 nopad borderunopen"></div>
                                                                        <div class="collapsablediv col-lg-12 col-md-12 col-sm-12 nopad borderopen">
                                                                            <div class="ulheadmorestyle nopad">
                                                                                {% for segItem in segmentsItem.segments %}
                                                                                    {% if loop.index>1 %}
                                                                                        <div class="width100 blackline"></div>
                                                                                    {% endif %}
                                                                                    {% for segItemIn in segItem %}
                                                                                        {% set segItemInIndex1 = loop.index %}
                                                                                        {% if segItemInIndex1>1 %}
                                                                                            <div class="wrap"><div class="text">{{segItemIn.stop_city|raw}}</div></div>
                                                                                            {% endif %}                                                             
                                                                                            {% if segItemIn.operating_airline != segItemIn.airline %}
                                                                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xm-12 textoperated"><span>{{'operated by'|trans}} {{segItemIn.operating_airline_name|raw}}</span></div>
                                                                                        {% endif %} 
                                                                                        <ul class="ulheadstyle2 width100">
                                                                                            <li class="airlinestyle2 newstyleairline">
                                                                                                <ul>
                                                                                                    <li> <img class="chtimg" alt="{{segItemIn.airline_name|raw}}" src="{{segItemIn.airline_logo|raw}}"/></li>
                                                                                                    <li>{{segItemIn.airline_name|raw}}<br>{{segItemIn.airline|raw}}{{segItemIn.flight_number|raw}}</li>
                                                                                                </ul>
                                                                                            </li>
                                                                                            <li class="departuredate newdep">{{segItemIn.departure_date_time|raw}}</li>
                                                                                            <li class="stpsstyle newstops"><img class="stpsimg" src="{{ generateMediaURL('/media/images/stpsimg.png') }}" alt="{{'stops'|trans}}"/><br> <span class="destpadr">{{segItemIn.departure_airport_code|raw}}</span> <span>{{segItemIn.arrival_airport_code|raw}}</span></li>
                                                                                            <li class="othercommonstyle newarrives">{{segItemIn.arrival_date_time|raw}}{#<span class="hourstyle"> 01:00</span><br><span class="plustime">+1 day</span>#}</li>
                                                                                        </ul>
                                                                                        <input type="hidden" name="flight_number-{{ segItemIn.segment_count }}" value="{{ segItemIn.flight_number|raw }}" />
                                                                                        <input type="hidden" name="airline_code-{{ segItemIn.segment_count }}" value="{{ segItemIn.airline|raw }}" />
                                                                                        <input type="hidden" name="operating_airline_code-{{ segItemIn.segment_count }}" value="{{ segItemIn.operating_airline|raw }}" />
                                                                                        <input type="hidden" name="airline_name-{{ segItemIn.segment_count }}" value="{{ segItemIn.airline_name|raw }}" />
