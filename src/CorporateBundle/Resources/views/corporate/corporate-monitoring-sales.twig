{% extends 'basecorporateFinal.html.twig' %}

{% block pagenewcss %}
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/corporate/css/corporate-common_new.css")|raw }}" >
{% endblock %}
{% block pagenewjs %}
{% endblock %}
{% block title %}{{'Monitoring Sales'|trans}}{% endblock %}
{% block body %}
    <section class="container corporate_common_new_container">
        <nav class="navbar navbar-default navbar-fixed-bottom tt-bottom-nav visible-sm visible-xs">
            <div class="container-fluid nopad filters-group">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bottomNavBar">
                        {{'Filters'|trans}}
                    </button>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse filter-options" id="bottomNavBar">
                    <div class="row no-margin">
                        <div class="col-xs-12 nopad">
                            {% include '@Corporate/corporate/monitoring_left_panel.twig' %}
                        </div>
                        <!-- /.navbar-collapse -->
                    </div>
                </div>
            </div>
            <!-- /.container-fluid -->
        </nav>
        <div class="row no-margin">
            <div class="col-xs-12 nopad">
                <div class="row no-margin margin_top_20 margin_bottom_40">
                    <div class="col-xs-12 nopad">
                        <div class="circlimg_arrow"><i class="fas fa-angle-right"></i></div>
                        <h1 class="search_title">{{'Monitoring'|trans}}</h1>
                    </div>
                </div>
                <div class="row no-margin bordered_row"></div>
            </div>
            <div class="col-xs-12">
                <div class="row margin_top_60">
                    <div class="col-md-3 col-xs-12 left_panel hidden-sm hidden-xs">
                        {% include '@Corporate/corporate/monitoring_left_panel.twig' %}
                    </div>
                    <div class="col-md-9 col-xs-12 right_panel">
                        <div class="row no-margin">
                            <div class="col-xs-12 nopad sort_by_tabs">
                                <div class="filters-group">
                                    <div class="col-xs-12 nopad btn-group sort-options">
                                        <a class="active" rel="nofollow" href="#" title="{{'Sales'|trans}}">
                                            <label class="sort_item">{{'Sales'|trans}}</label>
                                            <div class="absolutetooltip_pink_container">
                                                <img class="absolutetooltip_pink" src="/images/corporate_new/tooltip_pink.png" alt="tooltip_pink">
                                            </div>
                                        </a>
                                        <a rel="nofollow" href="#" title="{{'Customers'|trans}}">
                                            <label class="sort_item">{{'Customers'|trans}}</label>
                                            <div class="absolutetooltip_pink_container">
                                                <img class="absolutetooltip_pink" src="/images/corporate_new/tooltip_pink.png" alt="tooltip_pink">
                                            </div>
                                        </a>
                                        <a rel="nofollow" href="#" title="{{'Payments'|trans}}">
                                            <label class="sort_item">{{'Payments'|trans}}</label>
                                            <div class="absolutetooltip_pink_container">
                                                <img class="absolutetooltip_pink" src="/images/corporate_new/tooltip_pink.png" alt="tooltip_pink">
                                            </div>
                                        </a>
                                        <a rel="nofollow" href="#" title="{{'All bookings'|trans}}">
                                            <label class="sort_item">{{'All bookings'|trans}}</label>
                                            <div class="absolutetooltip_pink_container">
                                                <img class="absolutetooltip_pink" src="/images/corporate_new/tooltip_pink.png" alt="tooltip_pink">
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12 nopad">
                                <img class="width-100" src="/images/corporate_new/monitoring_img.jpg" alt="monitoring_img">
                            </div>
                            <div class="col-xs-12 nopad margin_top_50">
                                <table style="width:100%">
                                    <tr>
                                        <th>table title</th>
                                        <th>table title</th> 
                                        <th>table title</th>
                                    </tr>
                                    <tr>
                                        <td>table rows</td>
                                        <td>table rows</td>
                                        <td>table rows</td>
                                    </tr>
                                    <tr>
                                        <td>table rows</td>
                                        <td>table rows</td>
                                        <td>table rows</td>
                                    </tr>
                                    <tr>
                                        <td>table rows</td>
                                        <td>table rows</td>
                                        <td>table rows</td>
                                    </tr>
                                </table>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock %}
