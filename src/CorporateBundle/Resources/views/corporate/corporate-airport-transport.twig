			<input type="hidden" id="typeOfTransfer" name="typeOfTransfer" value="roundtrip" />
			<p class="titlest0black_sts"><span class="step0pinktext_sts"> {{'Step2'|trans}} /</span> {{'Choose your type of transfer'|trans}}</p>
			<div class="step1typetransfer active" onclick="changeTypeOfTransfer(this, 'roundtrip');">
			    <p class="bluesmtitle_sts">{{'Roundtrip'|trans}}</p>
			    <p class="textunblack_sts">{{'From Airport / Station / Port to Dropoff point and back'|trans}}</p>
			</div>
			<div class="step1typetransfer" onclick="changeTypeOfTransfer(this, 'oneWayFromAirport');">
			    <p class="bluesmtitle_sts">{{'One way from Airport'|trans}}</p>
			    <p class="textunblack_sts">{{'From Airport / Station / Port to Dropoff point'|trans}}</p>
			</div>
			<div class="step1typetransfer" onclick="changeTypeOfTransfer(this, 'oneWayToAirport');">
			    <p class="bluesmtitle_sts">{{'One way to Airport'|trans}}</p>
			    <p class="textunblack_sts">{{'From Pickup point to Airport / Station / Port'|trans}}</p>
			</div>
		    </div>
		    <div class="col-xs-12 nopad">
			<a id="step2previous_sts" class="nextsteps_sts">{{'previous step'|trans}}</a>
			<a id="step2next_sts" class="nextsteps_sts">{{'next step'|trans}}</a>
		    </div>
		</div>


		<!-- Start of Step 3 -->
		<div id="step-3_sts" class="hiddensteps_sts col-xs-12 nopad">
		    <div class="col-xs-12 nopad">
			<input type="hidden" id="transferAirportName" name="transferAirportName" value="roundtrip" />
			<p class="titlest0black_sts"><span class="step0pinktext_sts"> {{'Step3'|trans}} /</span> {{'Select your Airport / Port / Train Station'|trans}}</p>
		    </div>
		    <div id="step-3_sts_box" class="col-xs-12 nopad">
			&nbsp;
		    </div>
		    <div class="col-xs-12 nopad steptwostyle_sts">
			<a id="step3previous_sts" class="nextsteps_sts">{{'previous step'|trans}}</a>
			<a id="step3next_sts" class="nextsteps_sts">{{'next step'|trans}}</a>
		    </div>
		</div>

		<!-- Start of Step 4 -->
		<div id="step-4_sts" class="hiddensteps_sts col-xs-12 nopad">
		    <div class="col-xs-12 nopad">
			<p class="titlest0black_sts"><span class="step0pinktext_sts"> {{'Step4'|trans}} /</span>  {{'Enter your destination hotel or address'|trans}}</p>
			<select id="address" name="address" class="selectboxlargestandard_sts marg-bottom5">
			    <option value="" selected="selected" >{{'Destination hotel or address'|trans}}</option>
			    <option value="Westin Paris 3 Rue de Castiglione, Paris">{{'Westin Paris'|trans}}</option>
			    <option value="Bar Tuileries 3 Rue de Castiglione, Paris">{{'Bar Tuileries'|trans}}</option>
			    <option value="Hotel Dabicam Paris SNC 3 Rue de Castiglione, Paris">{{'Hotel Dabicam Paris SNC'|trans}}</option>
			</select> 
		    </div>
		    <div class="col-xs-12 nopad">
			<div class="hotelpinkcontainer">
			    <div class="hoteltable_attraction">
				<ul class="borderedtablelist">
				    <li class="pinkhederlist_att">{{'Hotels'|trans}}</li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild bluetextwizback">{{'The Westin Paris - Vendome'|trans}}</p>
					    <p class="listshild greydowntext">{{'3 Rue de Castiglone, Paris'|trans}}</p>
					</div>
				    </li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild bluetextwizback">{{'Westin Paris'|trans}}</p>
					    <p class="listshild greydowntext">{{'3 Rue de Castiglione, Paris'|trans}}</p>
					</div>
				    </li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild bluetextwizback">{{'Bar Tuileries'|trans}}</p>
					    <p class="listshild greydowntext">{{'3 Rue de Castiglione, Paris'|trans}}</p>
					</div>
				    </li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild bluetextwizback">{{'Hotel Dabicam Paris SNC'|trans}}</p>
					    <p class="listshild greydowntext">{{'3 Rue de Castiglione, Paris'|trans}}</p>
					</div>
				    </li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild bluetextwizback">{{'Le Meridien Etoile'|trans}}</p>
					    <p class="listshild greydowntext">{{'81 Boulevard Gouvion-Saint-Cyr, Paris'|trans}}</p>
					</div>
				    </li> 
				</ul>
			    </div>
			</div>
		    </div>
		    <div class="col-xs-12 nopad marbut20">
			<div class="hotelpinkcontainer">
			    <div class="hoteltable_attraction">
				<ul class="borderedtablelist">
				    <li class="pinkhederlist_att">{{'Addresses'|trans}}</li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild greyadresswizback">{{'Rue Westinghouse, Le Havre, France'|trans}}</p>
					</div>
				    </li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild greyadresswizback">{{'Boulevard Westinghouse, Sevra, France'|trans}}</p>
					</div>
				    </li> 
				    <li>
					<div class="listtextcontainer">
					    <p class="listshild cntfindboldblack">{{'Canâ€™t find your destination? Click here'|trans}}</p>
					</div>
				    </li> 
				</ul>
			    </div>
			</div>
		    </div> 
		    <div class="col-xs-12 nopad">
			<a id="step4previous_sts" class="nextsteps_sts">{{'previous step'|trans}}</a>
			<a id="step4next_sts" class="nextsteps_sts">{{'next step'|trans}}</a>
		    </div>
		</div>
		
		<!-- Start of Step 5 -->
		<div id="step-5_sts" class="hiddensteps_sts col-xs-12 nopad">
		    <input type="hidden" id="step5Cleared" name="step5Cleared" value="No" />
		    <div class="col-xs-12 nopad">
			<p class="titlest0black_sts"><span class="step0pinktext_sts"> {{'Step5'|trans}} /</span> {{'Select your schedule'|trans}}</p>
		    </div>
		    <div id="arrivalStep" class="col-xs-12 nopad hideField">
			<p class="pinkarrdeptext_att">{{'Your arrival schedule'|trans}}</p>
			<p class="bluetextlabel_att">{{'Enter your arrival date'|trans}}</p>
			<input id="arrivalInput" name="arrivalInput" onchange="updateArrivalAndDeparture();" placeholder="{{'Enter your arrival date'|trans}}" class="inputlargestandard_sts" type="date">
			<p class="bluetextlabel_att">{{'Enter your arrival time'|trans}}</p>
			<select id="arrivalHour" name="arrivalHour" onchange="updateArrivalAndDeparture();" class="selectboxsmalstandard_sts">
			    <option value="00" selected="selected" >{{'hours'|trans}}</option>
			    <option value="00">12am</option>
			    {% for i in 1..23 %}
				{% set hv = (i > 12) ? (i - 12) : i %}
				{% set hx = (i > 11) ? 'pm' : 'am' %}
				<option value="{{ '%02d'|format(i) }}">{{hv}}{{hx}}</option>
			    {% endfor %}
			</select>
			<select id="arrivalMinute" name="arrivalMinute" onchange="updateArrivalAndDeparture();" class="selectboxsmalstandard_sts">
			    <option value="00" selected="selected" >{{'minutes'|trans}}</option>
			    {% for i in 0..11 %}
				{% set m  = i * 5 %}
				<option value="{{ '%02d'|format(m) }}">{{ '%02d'|format(m) }}</option>
			    {% endfor %}
			</select>
		    </div>
		    <div class="seperatedborderdiv"></div>
		    <div id="departureStep" class="col-xs-12 nopad hideField">
			<p class="pinkarrdeptext_att">{{'Your departure schedule'|trans}}</p>
			<p class="bluetextlabel_att">{{'Enter your departure date'|trans}}</p>
			<input id="departureInput" name="departureInput" onchange="updateArrivalAndDeparture();" placeholder="{{'Enter your departure date'|trans}}" class="inputlargestandard_sts" type="date">
			<p class="bluetextlabel_att">{{'Enter your departure time'|trans}}</p>
			<select id="departureHour" name="departureHour" onchange="updateArrivalAndDeparture();" class="selectboxsmalstandard_sts">
			    <option value="00" selected="selected" >{{'hours'|trans}}</option>
			    <option value="00">12am</option>
			    {% for i in 1..23 %}
				{% set hv = (i > 12) ? (i - 12) : i %}
				{% set hx = (i > 11) ? 'pm' : 'am' %}
				<option value="{{ '%02d'|format(i) }}">{{hv}}{{hx}}</option>
			    {% endfor %}
			</select>
			<select id="departureMinute" name="departureMinute" onchange="updateArrivalAndDeparture();" class="selectboxsmalstandard_sts">
			    <option value="00" selected="selected" >{{'minutes'|trans}}</option>
			    {% for i in 0..11 %}
				{% set m  = i * 5 %}
				<option value="{{ '%02d'|format(m) }}">{{ '%02d'|format(m) }}</option>
			    {% endfor %}
			</select>
