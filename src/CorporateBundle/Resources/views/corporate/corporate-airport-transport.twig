			<select id="numOfpassengers" name="numOfpassengers" class="selectboxsmalstandard_sts">
			    <option value="0" selected="selected" >{{'Choose the'|trans}} #</option>
			    {% for i in 1..99 %}
				<option value="{{i}}">{{i}}</option>
			    {% endfor %}
			</select>
		    </div>
		    <div class="col-xs-12 nopad">
			<a id="step6previous_sts" class="nextsteps_sts">{{'previous step'|trans}}</a>
			<a id="step6next_sts" class="nextsteps_sts">{{'next step'|trans}}</a>
		    </div>
		</div>
		
		<!-- Start of Step 7 -->
		<div id="step-7_sts" class=" hiddensteps_sts col-xs-12 nopad">
		    <div class="col-xs-12 nopad">
			<p class="titlest0black_sts"><span class="step0pinktext_sts"> {{'Step7'|trans}} /</span> {{'Select your type of transfer'|trans}}</p>
		    </div>
		    <div class="imageandtextcontainer">
			<div class="col-xs-12 nopad">
			    <img class="imgleftstyle_airtrans" src="{{ generateMediaURL('/media/images/stepseven/stepsiximg1.png') }}" alt="{{'title'}}"/>
			    <div class="textcontainerright">
				<p class="pinktext_airtrans">{{'Shared'|trans}}</p>
				<p class="bluetext_airtrans">{{'Mercedes Benz or equivlent'|trans}}</p>
				<p class="blacktext_airtrans">{{'An affordable, convenient and secure solution'|trans}}</p>
				<a class="pinkbutacontainer_airtrans" href="#">US$ 89.82 - {{'add to cart'|trans}}</a>
			    </div>
			</div>
			<div class="seperatedborderdivlarge"></div>
			<div class="col-xs-12 nopad">
			    <img class="imgleftstyle_airtrans" src="{{ generateMediaURL('/media/images/stepseven/stepsiximg2.png') }}" alt="{{'title'}}"/>
			    <div class="textcontainerright">
				<p class="pinktext_airtrans">{{'Private'|trans}}</p>
				<p class="bluetext_airtrans">{{'Mercedes Benz or equivlent'|trans}}</p>
				<p class="blacktext_airtrans">{{'A comfortable, deluxe option with a dedicated driver 
				    ready for your safe transfer.'|trans}}</p>
				<a class="pinkbutacontainer_airtrans" href="#">US$ 89.82 - {{'add to cart'|trans}}</a>
			    </div>
			</div>
		    </div>
		    <div class="col-xs-12 nopad">
			<a id="step7previous_sts" class="nextsteps_sts">{{'previous step'|trans}}</a>
			<a id="step7next_sts" class="nextsteps_sts">{{'next step'|trans}}</a>
		    </div>
		</div>
