			<p class="titlest0black_sts"><span class="step0pinktext_sts"> {{'Step0'|trans}} /</span> {{'Choose your destination country'|trans}}</p>
			<select id="country" name="country" class="selectboxlargestandard_sts">
			    <option value="" selected="selected" >{{'Select your destination Country'|trans}}</option>
			    {% for key,val in countries %}
				<option value="{{val['code']}}">{{val['name']}}</option>
			    {% endfor %}
			</select> 
		    </div>
		    <div class="col-xs-12 nopad">
			<a id="step0next_sts" class="nextsteps_sts">{{'next step'|trans}}</a>
		    </div>
		</div>
		
		
		<!-- Start of Step 1 -->
