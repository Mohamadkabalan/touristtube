                                            <label for="refundableFilter"><input id="refundableFilter" type="checkbox" value="" data-filter=".refundable"/>{{'refundable only'|trans}}</label>
                                        </div>
                                    {% endif %}
                                </div>

                                <div class="accordion-section">
                                    <div class="block_r airline-f ">
                                        <div class="title_with_icon">
                                            <div class="bg fl"></div>
                                            <div class="txt fl accordion-section-title">{{'airlines'|trans}}</div>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class=" accordion-section-content nopad">
                                            <div id="airlinesGroup" class="checkbox input-group" data-filter-group="airlines-group">
                                                {% for key, airline in airlines %}
                                                    <label for="{{ key|raw }}">
                                                        <input id="{{ key|raw }}" type="checkbox" value="{{ key|raw }}" data-filter=".{{ key|raw }}">{{ airline.name|raw }} <span class="a_price price-convert" data-price="{{ airline.amount_attr|raw }}"><span class="currency-convert-text">{{currency_code|raw}}</span> <span class="price-convert-text">{{ airline.amount|raw }}</span></span>
                                                    </label>
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
