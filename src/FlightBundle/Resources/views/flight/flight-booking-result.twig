                                                    <div class="price-convert" data-price="{{segmentsItem.base_fare_attr|raw}}"><input class="price-convert-val" type="hidden" name="base_fare_attr" value="{{ segmentsItem.base_fare_attr|raw }}" /></div>
                                                    <input type="hidden" name="original_taxes" value="{{ segmentsItem.original_taxes|raw }}" />
                                                    <div class="price-convert" data-price="{{segmentsItem.taxes|raw}}"><input class="price-convert-val" type="hidden" name="taxes" value="{{ segmentsItem.taxes|raw }}" /></div>
                                                    <div class="price-convert" data-price="{{segmentsItem.taxes_attr|raw}}"><input class="price-convert-val" type="hidden" name="taxes_attr" value="{{ segmentsItem.taxes_attr|raw }}" /></div>
                                                        {% for passenger in segmentsItem.passenger_info %}
                                                        <input type="hidden" name="{{ passenger.type_code }}" value="{{ passenger.quantity|raw }}" />
                                                        <input type="hidden" name="leaving_baggage_info_{{ passenger.type_code }}" value="{{ passenger.leaving_baggage_info|raw }}" />
                                                        {% if passenger.returning_baggage_info is defined %}
                                                            <input type="hidden" name="returning_baggage_info_{{ passenger.type_code }}" value="{{ passenger.returning_baggage_info|raw }}" />
                                                        {% endif %}
                                                        <input type="hidden" name="fare_calc_line_{{ passenger.type_code }}" value="{{ passenger.fare_calculation_line|raw }}" />
                                                    {% endfor %}
                                                    <input type="hidden" name="sec_token" value="{{ segmentsItem.sec_token|raw }}" />
                                                </form>
                                            </div>
                                        {% endfor %}
                                    {% else %}
                                        <div class="alert-danger search-result">{{'There are no flights that match your search criteria.'|trans}}</div>
                                    {% endif %}
                                {% endif %}
                                <div class="no-filter-result alert-danger search-result">{{'No flights match your filters.'|trans}}</div>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock %}
