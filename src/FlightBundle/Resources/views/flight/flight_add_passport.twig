                            <div class="col-xs-12">
                                <div class="error form-errors"></div>
                                <div class="row margin_top_30">
                                    <div class="col-xs-12">
                                        <i class="fas fa-angle-right fa-lg tt-brand-color"></i> <span class="font-24 txt-black">{{ 'Traveler'|trans}} {{ loop.index }}</span> {{ personalInfo.type|trans }} {% if loop.index == 1 %}, {{'primary contact'|trans}} {% endif %}
                                        <input type="hidden" id="passengerNameRecord_passengerDetails_0_type" name="passengerNameRecord[passengerDetails][0][type]" value="ADT" value="ADT" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4 col-sm-6 col-xs-12 margin_top_15">
                                        <label for="name">{{ 'First Name'|trans }}</label>
                                        <input type="text" id="passengerNameRecord_passengerDetails_0_firstName" name="passportInfo[{{ personalInfo.passengerNumber }}][firstName]" required="required" class="flight_input required" placeholder="First name*" value="{{ personalInfo.firstName }}" readonly/>
