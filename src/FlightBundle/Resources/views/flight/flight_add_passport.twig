{% extends 'baseFinal.html.twig' %}

{% block pagenewcss %}
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/flights/css/book-flights_new.css")|raw }}" >
{% endblock %}

{% block pagenewjs %}
    <script type="text/javascript">
        var isCorporate = false;
    </script>
{% endblock %}

{% block title %}{{ 'Passport'|trans }} | Tourist Tube{% endblock %}

{% block body %}
    <section class="container main_flight_container flight_add_passport">
        <form name="passengerNameRecord" method="post" action="/flight-review-trip" id="bookAirTicket">
            <div class="row margin_top_30">
                <div class="col-xs-12">
                    <h1 class="search_title"><span class="circlimg_arrow"><i class="fas fa-angle-right"></i></span>{{ 'Passport'|trans }}</h1>
                </div>
            </div>

            {% set _cnn = '' %}
            {% if tickets.count > 1 %}
                {% set _label = 'tickets'|trans %}
                {% set _adt = tickets.ADT|length ~ ' Adults'|trans %}

                {% if tickets.CNN is defined %}
                    {% set _cnn = tickets.CNN|length ~ ' Children'|trans %}
                {% endif %}
            {% else %}
                {% set _label = 'ticket'|trans %}
                {% set _adt = 'Adult'|trans %}
            {% endif %}

            <div class="row margin_top_30">
                <div class="col-md-3 col-xs-12 left_panel">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="icon-list"><i class="fas fa-angle-right fa-2x"></i></div>
                            <div class="side-list">
                                <h2>{{ flight_type|trans }} {{ 'Flight'|trans }}</h2>
                                <p class="hidden-sm"><span class="txt-black">{{ passengerDetails.departureAirport }} to {{ passengerDetails.arrivalAirport }}</span></p>
                                <p class="hidden-sm">{{ passengerDetails.departureDate }} <span class="visible-xs">{{ tickets.count }} {{ _label }}: {{ _adt }} {{ _cnn }}</span></p>
                                <p class="hidden-sm hidden-xs">{{ tickets.count }} {{ _label }}: {{ _adt }} {{ _cnn }} </p>
                            </div>
                        </div>
                        <div class="col-xs-12 margin_top_40 small_margin_top_10">
                            <div class="icon-list"><i class="fas fa-angle-right fa-2x"></i></div>
                            <div class="side-list">
                                <div class="row">
                                    <div class="col-md-12 col-sm-3 col-xs-6">
                                        <h2 class="margin_bottom_0">Price</h2>
                                    </div>
                                    <div class="col-md-12 col-sm-3 col-xs-6">
                                        <p class="margin_bottom_0">{{ 'Base Fare'|trans }}</p>
                                        <p class="txt-black margin_top_0">{{ passengerDetails.displayCurrency }} {{ passengerDetails.displayBaseFare }}</p>
                                    </div>

                                    <div class="visible-xs clearfix"></div>

                                    <div class="col-md-12 col-sm-3 col-xs-6">
                                        <p class="margin_bottom_0">{{ 'Taxes and fees'|trans }}</p>
                                        <p class="txt-black margin_top_0">{{ passengerDetails.displayCurrency }} {{ passengerDetails.displayTaxes }}</p>
                                    </div>
                                    <div class="col-md-12 col-sm-3 col-xs-6 txt-black">
                                        <p class="margin_bottom_0">{{ 'Flight total'|trans }}</p>
                                        <p class="margin_top_0 font-30">{{ passengerDetails.displayCurrency }} {{ passengerDetails.displayOriginalAmount }}</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-9 col-xs-12 right_panel">
                    <div class="row small_margin_top_10">
                        <div class="col-md-10 col-xs-12">
                            <div class="icon-list-exclamation"><i class="fas fa-exclamation fa-lg"></i></div>
                            <div class="side-list">
                                {{ 'Rates can change frequently.'|trans }}<br/>
                                {{ 'Book now to lock in this price.'|trans }}
                            </div>
                        </div>
                        <div class="col-xs-12"><div class="border-bottom-2 margin_top_15"></div></div>
                    </div>

                    <div class="row">
                        <div class="col-xs-12">
                            <h2 class="txt-black">{{'Who'|trans}}&#039;{{'s flying'|trans}}?</h2>
                            <p>{{'Traveler names must match your government-issued photo ID exactly.'|trans}}</p>
                            <p>{{ passengerDetails.departureAirport }} {{'to'|trans}} {{ passengerDetails.arrivalAirport }} <span class="txt-dark-grey">>

                                    {{ passengerDetails.departureDate }}
                                </span></p>
                        </div>
                    </div>
                    {% if passengerDetails.success == true %}
                    <input type="text"  name="transactionId" class="flight_input required" placeholder="Passport No." value="{{ passengerIdentifier.transactionId }}" hidden>
                    <input type="text"  name="pnr" class="flight_input required" placeholder="Passport No." value="{{  passengerIdentifier.pnr }}" hidden>
                    {% for personalInfo in passengerDetails.data %}
