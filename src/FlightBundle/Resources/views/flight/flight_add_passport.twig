                                        <input type="text" id="passengerNameRecord_passengerDetails_0_passportNo" name="passportInfo[{{ personalInfo.passengerNumber }}][gender]" class="flight_input required" placeholder="Passport No." value="{{ personalInfo.gender }}" hidden>
                                        <div class="error"></div>
                                    </div>
                                    <div class="col-md-4 col-sm-6 col-xs-12 margin_top_15">
                                        <label for="lastname">{{ 'Last Name'|trans }}</label>
                                        <input type="text" id="passengerNameRecord_passengerDetails_0_surname" name="passportInfo[{{ personalInfo.passengerNumber }}][lastName]" required="required" class="flight_input required" placeholder="Last name*" value="{{ personalInfo.lastName }}" readonly/>
                                        <div class="error"></div>
                                    </div>
                                    <div class="col-md-4 col-sm-6 col-xs-12 margin_top_15">
                                        <label for="dab">{{ 'Date of birth'|trans }}</label>
                                        <input type="text" id="passengerNameRecord_passengerDetails_0_firstName" name="passportInfo[{{ personalInfo.passengerNumber }}][dateOfBirth]" readonly class="flight_input required" placeholder="First name*" value="{{ personalInfo.dob|date("Y-m-d") }}" />
