{% if (myBookings) %}
{% for travel in myBookings %}
<div class="col-xs-12 nopad">
	<!-- start of hotels -->
	{% if travel.moduleId == hotelModuleId %}
	<div class="row no-margin">
		<div id="my-bookings-hotels" class="col-xs-12 nopad">
			{% include 'mybookings/my-bookings-hotel-new-loop.twig' %}
		</div>
	</div>
	<!-- end of hotels -->
	{% elseif travel.moduleId == flightModuleId %}
	<!-- start of flights -->
	<div class="row no-margin">
		<div id="my-bookings-flight" class="bookDataContainer">
			{% include 'mybookings/my-bookings-flight-new-loop.twig' %}
		</div>
	</div>
	{% elseif travel.moduleId == dealModuleId %}
	<!-- start of flights -->
	<div class="row no-margin">
		<div id="my-bookings-deals" class="bookDataContainer">
			{% include 'mybookings/my-bookings-deals-new-loop.twig' %}
		</div>
	</div>
	{% endif %}
	<!-- end of flights -->
</div>
{% endfor %}
{% else %}
<div class="row no-margin">
	<div class="col-xs-12 nopad">
		<p class="leftmenue_subtitles">
			{{'No Records found'|trans}}
		</p>
	</div>
</div>
{% endif %}
